                    <button type="button" class="btn-wrap cart-link d-flex align-items-center">View Product &nbsp;&nbsp;&nbsp;<i class="icon icon-arrow-io"></i>
                    </button>

                  </div>
                </div>

                </a>

                <div class="product-detail">
                  <h3 class="product-title">
                    <a href="/product_info/<%- trending[i].product_id %>"><%- trending[i].product_name %></a>
