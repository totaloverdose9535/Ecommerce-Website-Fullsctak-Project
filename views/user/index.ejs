<%- include("navbar.ejs") %>

    <section id="billboard" class="overflow-hidden">

      <button class="button-prev">
        <i class="icon icon-chevron-left"></i>
      </button>
      <button class="button-next">
        <i class="icon icon-chevron-right"></i>
      </button>
      <div class="swiper main-swiper">
        <div class="swiper-wrapper">
          <% for(var i=0;i<slides.length;i++){ %>
          <div class="swiper-slide" style="background-image: url('/uploads/<%- slides[i].slide_image %>');background-repeat: no-repeat;background-size: cover;background-position: center;">
            <div class="banner-content">
              <div class="container">
                <div class="row">
                  <div class="col-md-6">
                    <h2 class="banner-title"><%- slides[i].slide_title %></h2>
                    <p><%- slides[i].slide_details %></p>
                    <div class="btn-wrap">
                      <a href="<%- slides[i].button_link %>" class="btn btn-light btn-medium d-flex align-items-center" tabindex="0"><%- slides[i].button_text %> <i class="icon icon-arrow-io"></i>
                      </a>
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </div>
          <% } %>
          <div class="swiper-slide" style="background-image: url('images/banner2.jpg');background-repeat: no-repeat;background-size: cover;background-position: center;">
            <div class="banner-content">
              <div class="container">
                <div class="row">
                  <div class="col-md-6">
                    <h2 class="banner-title">Casual Collection</h2>
                    <p>Lorem ipsum dolor sit amet, consectetur adipiscing elit. Sed eu feugiat amet, libero ipsum enim pharetra hac.</p>
                    <div class="btn-wrap">
                      <a href="/shop" class="btn btn-light btn-light-arrow btn-medium d-flex align-items-center" tabindex="0">Shop it now <i class="icon icon-arrow-io"></i>
                      </a>
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </section>

    <section id="featured-products" class="product-store padding-large">
      <div class="container">
        <div class="section-header d-flex flex-wrap align-items-center justify-content-between">
          <h2 class="section-title">Featured Products</h2>            
          <div class="btn-wrap">
            <a href="/shop" class="d-flex align-items-center">View all products <i class="icon icon icon-arrow-io"></i></a>
          </div>            
        </div>
        <div class="swiper product-swiper overflow-hidden">
          <div class="swiper-wrapper">
            <div class="swiper-slide">


              <% for(var i=0;i<featured.length;i++) { %>
              <div class="product-item">



                <div class="image-holder">
                  <img src="/uploads/<%- featured[i].product_image1 %>" alt="Books" class="product-image" style="height: 300px; width: 400px;">
                </div>

                <a href="/product_info/<%- featured[i].product_id %>">

                <div class="cart-concern">
                  <div class="cart-button d-flex justify-content-between align-items-center">
                    
                    <button type="button" class="btn-wrap cart-link d-flex align-items-center">View Product &nbsp;&nbsp;&nbsp;<i class="icon icon-arrow-io"></i>
                    </button>

                  </div>
                </div>

                </a>

                <div class="product-detail">
                  <h3 class="product-title">
                    <a href="/product_info/<%- featured[i].product_id %>"><%- featured[i].product_name %></a>
                  </h3>
                  <div class="item-price text-primary">&#8377; <%- featured[i].price%> <del style="color: black;font-size: 20px;">&nbsp;&#8377;<%-featured[i].duplicate_price %>&nbsp;</del></div>
                </div>
              </div>
              <% } %> 
            </div>

          </div>
        </div>
        <div class="swiper-pagination"></div>
      </div>
    </section>

    <section id="trending-products" class="product-store" style="margin-top: -10%;">
      <div class="container">
